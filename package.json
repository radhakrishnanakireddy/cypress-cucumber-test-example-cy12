{
  "name": "cypress-with-cucumber",
  "version": "1.0.0",
  "description": "This is a simple project that demonstrates end-to-end testing with using cypress.io",
  "main": "index.js",
  "devDependencies": {
    "cypress": "^9.2.1",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/step_definitions"
  },
  "scripts": {
    "test": "cypress run --spec **/*.feature",
    "cypress:open": "cypress open",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burakkaygusuz/cypress-with-cucumber.git"
  },
  "keywords": [
    "cypress",
    "cucumber"
  ],
  "author": "Burak Kaygusuz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/burakkaygusuz/cypress-with-cucumber/issues"
  },
  "homepage": "https://github.com/burakkaygusuz/cypress-with-cucumber#readme"
}
