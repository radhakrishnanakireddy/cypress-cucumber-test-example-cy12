  "bugs": {
    "url": "https://github.com/burakkaygusuz/cypress-cucumber-test-example/issues"
  },
  "homepage": "https://github.com/burakkaygusuz/cypress-cucumber-test-example#readme",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^9.2.1"
  }
