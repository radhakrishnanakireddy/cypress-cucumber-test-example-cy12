{
  "name": "cypress-cucumber-test-example",
  "version": "1.0.0",
  "description": "This is a simple project that demonstrates e2e testing with using cypress.io and cucumber.js",
  "main": "index.js",
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@cucumber/cucumber": "^8.2.1",
    "cypress": "^9.6.1",
    "cypress-mochawesome-reporter": "^3.1.0",
    "esbuild": "^0.14.38",
    "eslint": "^8.16.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "2.6.2"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/integration/step_definitions/**/*.{js,ts}"
  },
  "scripts": {
    "test": "cypress run --spec **/*.feature",
    "cypress:open": "cypress open",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burakkaygusuz/cypress-cucumber-test-example.git"
  },
  "keywords": [
