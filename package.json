{
  "name": "cypress-cucumber-test-example",
  "version": "1.0.0",
  "description": "This is a simple project that demonstrates e2e testing with using cypress.io and cucumber.js",
  "main": "index.js",
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@cucumber/cucumber": "^8.5.2",
    "@faker-js/faker": "^7.5.0",
    "cypress": "^10.8.0",
    "esbuild": "^0.15.5",
    "eslint": "^8.22.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.7.1"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/**/*.{js,ts}"
  },
  "scripts": {
    "test": "cypress run --e2e --browser chrome --spec **/*.feature",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burakkaygusuz/cypress-cucumber-test-example.git"
  },
  "keywords": [
    "cypress",
    "cucumber"
  ],
  "author": "Burak Kaygusuz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/burakkaygusuz/cypress-cucumber-test-example/issues"
  },
  "homepage": "https://github.com/burakkaygusuz/cypress-cucumber-test-example#readme",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.0"
  }
}
